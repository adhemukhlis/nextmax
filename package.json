{
	"name": "nextmax",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "env-cmd -x -f .env next dev -p \\${PORT}",
		"start": "env-cmd -x -f .env next start -p \\${PORT}",
