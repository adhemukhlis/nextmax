{
	"name": "nextmax",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "yarn env-cmd -x -f .env next dev --turbopack -p \\${PORT}",
		"start": "yarn env-cmd -x -f .env next start -p \\${PORT}",
		"start:turbo": "yarn env-cmd -x -f .env next start --turbopack -p \\${PORT}",
		"build": "next build",
		"build:turbo": "next build --turbopack",
		"lint": "eslint .",
		"typecheck": "tsc --noEmit",
		"fix": "eslint --fix .",
		"format": "yarn prettier --config .prettierrc --write ./src",
		"dependencies-upgrade": "yarn yarn-upgrade-all",
		"install:prod": "yarn install --production",
		"env-cmd-test": "yarn env-cmd -x -f .env echo \\${PORT}"
	},
	"dependencies": {
		"dayjs": "^1.11.18",
		"env-cmd": "^11.0.0",
		"next": "^15.5.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.38.0",
		"@types/node": "^24.8.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@typescript-eslint/eslint-plugin": "^8.46.1",
		"@typescript-eslint/parser": "^8.46.1",
		"eslint": "^9.37.0",
		"eslint-config-next": "^15.5.5",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-next": "^0.0.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.0",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"sass": "^1.92.1",
		"typescript": "^5.9.3",
		"yarn-upgrade-all": "^0.7.5"
	},
	"yarn-upgrade-all": {
		"ignore": [
			"sass"
		]
	}
}
